{
  "devDependencies": {
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3"
  },
  "name": "capim",
  "scripts": {
    "build": "npm ci && npm run generate --workspace main",
    "lint": "npm run lint --prefix main",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "p": "npm run local-client-to-remote-server-proxy --workspace main",
    "s": "npm run start-using-https --workspace main",
    "start": "npm run dev --workspace main",
    "test": "npm run test --workspace tests",
    "x": "npm run x --workspace main"
  },
  "workspaces": [
    "main",
    "tests"
  ]
}
